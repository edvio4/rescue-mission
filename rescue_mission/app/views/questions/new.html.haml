%h1 New Question
%div
  = form_for(@question) do |f|
    %div= f.label :title
    %div= f.text_field :title, placeholder: "Question Title"
    %div= f.label :description
    %div= f.text_area :description, placeholder: "Question Description"
    %div= render 'form'
%div= link_to 'Back', questions_path
